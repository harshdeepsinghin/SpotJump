{
  "manifest_version": 3,
  "name": "SpotJump",
  "version": "1.0",
  "description": "Jump from YouTube to Spotify instantly",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}